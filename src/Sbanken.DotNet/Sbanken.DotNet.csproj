<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <VersionPrefix>0.2.0</VersionPrefix>
    <Authors>Anders Austad</Authors>
    <Company />
    <Product></Product>
    <PackageTags></PackageTags>
    <PackageProjectUrl>https://github.com/anderaus/Sbanken.DotNet</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>git://github.com/anderaus/Sbanken.DotNet</RepositoryUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/anderaus/Sbanken.DotNet/master/assets/sbanken-dotnet-logo-512.png</PackageIconUrl>
    <Description>Unofficial .NET (C#) client library for Sbanken API</Description>
